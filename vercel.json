{
  "version": 2,
  "builds": [
    {
      "src": "ui/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://instabids-agent-swarm-8k5am.ondigitalocean.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ui/$1"
    }
  ]
}